    <context:property-placeholder location="restmule.properties" system-properties-mode="OVERRIDE"/>

    <flow name="HTTP input">
        <http:inbound-endpoint host="localhost" port="${port1}" path="parkingiq">
            <http:body-to-parameter-map-transformer />
            <response>
                <object-to-string-transformer />
            </response>
        </http:inbound-endpoint>
        <choice>
            <when expression="payload.method == 'REST'" evaluator="groovy">
                <flow-ref name="REST" />
            </when>
        </choice>
    </flow>

    <flow name="REST">
    </flow>

